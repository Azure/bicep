<?xml version="1.0" encoding="utf-8"?>
<VSTemplate Version="3.0.0" Type="Item" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" xmlns:sdk="http://schemas.microsoft.com/developer/vstemplate-sdkextension/2010">
  <TemplateData>
    <Name>Bicep File</Name>
    <Description>An empty Bicep template</Description>
    <TemplateID>Microsoft.Bicep.Web</TemplateID>
    <ProjectType>Web</ProjectType>
    <ProjectSubType>CSharp</ProjectSubType>
    <NumberOfParentCategoriesToRollUp>1</NumberOfParentCategoriesToRollUp>
    <DefaultName>main.bicep</DefaultName>
    <Icon>BicepItemTemplateIcon.png</Icon>
    <ShowByDefault>true</ShowByDefault>
  </TemplateData>
  <TemplateContent>
    <ProjectItem TargetFileName="$fileinputname$.bicep" ReplaceParameters="true">main.bicep</ProjectItem>
  </TemplateContent>
</VSTemplate>
