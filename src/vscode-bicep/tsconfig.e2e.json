// We need to compile the e2e tests until https://github.com/microsoft/vscode-test/issues/92 is closed.
{
  "compilerOptions": {
    "target": "es2018",
    "lib": ["es2018"],
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "outDir": "out/test",
    "sourceMap": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "jsx": "react-jsx"
  },
  "include": ["src/test/e2e"]
}
