    "myObj": {
      "a": "a",
      "b": -12,
      "c": true,
      "d": "[not(json('true'))]",
      "list": [
        1,
        2,
        "[add(2, 1)]",
        {
          "test": "[or(and(greater(144, 33), json('true')), lessOrEquals(99, 199))]"
        },
        "[equals(toLower('a'), toLower('b'))]"
      ],
      "obj": {
        "nested": [
          "hello"
        ]
      }
    },
    "myArr": [
      "pirates",
      "say",
      "arr"
    ],
    "myArrWithObjects": [
      {
        "name": "one",
        "enable": true
