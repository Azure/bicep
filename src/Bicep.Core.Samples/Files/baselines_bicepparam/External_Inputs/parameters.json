{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "singleParam": {
      "expression": "[externalInputs('sys_cli_0')]"
    },
    "singleParamCast": {
      "expression": "[bool(externalInputs('sys_cli_1'))]"
    },
    "foo": {
      "expression": "[externalInputs('sys_cli_2')]"
    },
    "bar": {
      "expression": "[externalInputs('sys_envVar_3')]"
    },
    "baz": {
      "expression": "[externalInputs('custom_binding_4')]"
    },
    "varRef": {
      "expression": "[bool(externalInputs('sys_cli_5'))]"
    },
    "objectConfig": {
      "expression": "[externalInputs('custom_tool_6')]"
    },
    "c": {
      "expression": "[externalInputs('sys_cli_7')]"
    },
    "a1": {
      "expression": "[externalInputs('sys_cli_8')]"
    },
    "b1": {
      "expression": "[externalInputs('sys_cli_8')]"
    },
    "c1": {
      "expression": "[externalInputs('sys_cli_8')]"
    },
    "first": {
      "expression": "[int(externalInputs('custom_binding_9'))]"
    },
    "second": {
      "expression": "[externalInputs('custom_binding_10')]"
    },
    "result": {
      "expression": "[format('{0} combined with {1}', int(externalInputs('custom_binding_9')), externalInputs('custom_binding_10'))]"
    },
    "myParam": {
      "expression": "[externalInputs('sys_cli_11')]"
    },
    "coolParam": {
      "expression": "[externalInputs('sys_sons_cool_param_provider_12')]"
    }
  },
  "externalInputDefinitions": {
    "sys_envVar_3": {
      "kind": "sys.envVar",
      "options": "bar"
    },
    "sys_cli_8": {
      "kind": "sys.cli",
      "options": "a"
    },
    "sys_cli_1": {
      "kind": "sys.cli",
      "options": "foo"
    },
    "sys_cli_0": {
      "kind": "sys.cli",
      "options": "foo"
    },
    "sys_cli_7": {
      "kind": "sys.cli",
      "options": "a"
    },
    "sys_sons_cool_param_provider_12": {
      "kind": "sys&sons.cool#param provider"
    },
    "sys_cli_11": {
      "kind": "sys.cli",
      "options": "myParam"
    },
    "sys_cli_2": {
      "kind": "sys.cli",
      "options": "foo"
    },
    "custom_binding_10": {
      "kind": "custom.binding",
      "options": {
        "path": "/path/to/file",
        "isSecure": true
      }
    },
    "sys_cli_5": {
      "kind": "sys.cli",
      "options": "myVar"
    },
    "custom_tool_6": {
      "kind": "custom.tool",
      "options": {
        "path": "/path/to/file",
        "isSecure": true
      }
    },
    "custom_binding_4": {
      "kind": "custom.binding",
      "options": "__BINDING__"
    },
    "custom_binding_9": {
      "kind": "custom.binding",
      "options": "__BINDING__"
    }
  }
}