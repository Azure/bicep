{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "type": "string",
      "defaultValue": "[resourceGroup().location]"
    },
    "scriptToExecute": {
      "type": "string",
      "defaultValue": "echo bicep"
    },
    "subId": {
      "type": "string",
      "defaultValue": "[subscription().id]"
    },
    "rgName": {
      "type": "string",
      "defaultValue": "[resourceGroup().name]"
    },
    "uamiName": {
      "type": "string",
      "defaultValue": "alex-test-deny"
    }
  },
  "functions": [],
  "variables": {
    "uamiId": "[resourceId(parameters('subId'), parameters('rgName'), 'Microsoft.ManagedIdentity/userAssignedIdentities', parameters('uamiName'))]"
  },
  "resources": [
    {
      "type": "Microsoft.Storage/storageAccounts",
      "apiVersion": "2019-06-01",
      "name": "[format('dscript{0}', uniqueString(resourceGroup().id))]",
      "location": "[parameters('location')]",
      "kind": "StorageV2",
      "sku": {
        "name": "Standard_LRS"
      }
    }
  ],
  "outputs": {
    "mgId": {
      "type": "string",
      "value": "[variables('uamiId')]"
    }
  }
}